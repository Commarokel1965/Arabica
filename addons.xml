<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="repository.supMC" name="supMC Addon Repository" version="2.3" provider-name="SupElectro">
	<extension point="xbmc.addon.repository" name="supMC Addon Repository">
		<info compressed="false">https://raw.githubusercontent.com/Commarokel1965/arabic/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/Commarokel1965/arabic/master/addons.xml.md5</checksum>
		<datadir zip="true">https://github.com/Commarokel1965/arabic</datadir>
	</extension>
</addon>

<addon id="plugin.video.livetv"
       name="LIVE TV"
       version="3.3.4"
       provider-name="SupElectro">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.beautifulsoup" version="3.0.8"/>
    <import addon="plugin.video.youtube" version="2.9.2"/>
	<import addon="script.module.elementtree" version="1.2.8"/>
	<import addon="script.common.plugin.cache" version="0.9.1"/>

  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Live TV</summary>
    <description>Live Arabic TV Link</description>
    <platform>all</platform>
  </extension>
</addon>

<addon
  id="plugin.video.movies"
  version="1.3.0"
  name="MOVIES"
  provider-name="SupElectro">
  <requires>
    <import addon="xbmc.python"
            version="1.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
             library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>MOVIES Arabic plugin</summary>
    <description>MOVIES plugin, Arabic , Hollywood, European and Bollywood movies </description>

    <platform>all</platform>
  </extension>
</addon>

<addon
  id="plugin.video.series"
  version="1.9.0"
  name="SERIES"
  provider-name="SupElectro">
  <requires>
    <import addon="xbmc.python"
            version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
             library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>vod Arabic plugin</summary>
    <description>Arabic plugin. Series</description>

    <platform>all</platform>
  </extension>
</addon>
</addons>
